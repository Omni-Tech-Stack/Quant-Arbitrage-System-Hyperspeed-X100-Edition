# Ultra-Fast Arbitrage Engine Configuration
# Copy this file to .env and fill in your values
# Run 'yarn setup' for interactive configuration

# Network Configuration
PRIMARY_RPC_ENDPOINT=https://polygon-rpc.com
CHAIN_ID=137

# Wallet Configuration
EXECUTOR_PRIVATE_KEY=your_private_key_here
EXECUTOR_ADDRESS=0xYourWalletAddress

# Protocol Endpoints (Polygon Mainnet)
UNISWAP_V2_ROUTER=0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff
UNISWAP_V3_ROUTER=0xE592427A0AEce92De3Edee1F18E0157C05861564
SUSHISWAP_ROUTER=0x1b02dA8Cb0d097eB8D57A175b88c7D8b47997506
QUICKSWAP_ROUTER=0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff

# MEV Configuration
FLASHBOTS_RELAY_URL=https://relay.flashbots.net
BLOXROUTE_AUTH_HEADER=
EDEN_ENDPOINT=
MEV_SHARE_ENABLED=true

# Database
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=omtegrate
POSTGRES_USER=omtegrate_user
POSTGRES_PASSWORD=your_db_password

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Monitoring & Alerts
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
SLACK_WEBHOOK_URL=
EMAIL_SMTP_HOST=
EMAIL_SMTP_PORT=587
EMAIL_FROM=
EMAIL_TO=

# Metrics
PROMETHEUS_PORT=9090
ENABLE_METRICS=true
HEALTH_CHECK_PORT=8080

# ML Configuration
ML_MODEL_PATH=./models/arb_ml_latest.pkl
ML_RETRAIN_INTERVAL=100
TAR_THRESHOLD=0.4
MIN_TRAINING_SAMPLES=1000

# Execution Parameters
MIN_PROFIT_USD=10.00
MAX_GAS_PRICE_GWEI=2100
SLIPPAGE_TOLERANCE=0.5
SIMULATION_REQUIRED=true
MAX_POSITION_SIZE_USD=100000

# Risk Management
MAX_BUNDLE_AGE_MS=12000
MAX_RETRY_ATTEMPTS=3
BLACKLIST_TOKENS=

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/omtegrate.log
LOG_MAX_SIZE=100M
LOG_MAX_FILES=30

# Multi-Chain Configuration
MULTI_CHAIN_ENABLED=true

# Chain toggles (set to true/false to enable/disable)
POLYGON_MAINNET=true
BASE_MAINNET=true
ARBITRUM_MAINNET=true
BSC_MAINNET=true
POLYGON_ZKEVM=true
FRAX_MAINNET=true
MOONBEAM_MAINNET=true

# Add chain-specific RPC endpoints as needed
# See full .env for all available chains
